<facelet-taglib xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd"
	version="2.2">
	<namespace>http://outjected.com/jsf/test</namespace>
	<tag>
		<description>Renders a simple Button</description>
		<tag-name>button</tag-name>
		<component>
			<component-type>test.outjected.components.ButtonComponent</component-type>
			<renderer-type>test.outjected.renderers.ButtonRenderer</renderer-type>
		</component>
		<attribute>
			<description>id</description>
			<name>id</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Text value of the button</description>
			<name>value</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>CSS Style </description>
			<name>style</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>CSS Style Classes</description>
			<name>styleClass</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>onclick</description>
			<name>onclick</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Type of the Button (submit,button,reset). Default is button</description>
			<name>type</name>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<description>Renders a copy component</description>
		<tag-name>copy</tag-name>
		<component>
			<component-type>test.outjected.components.CopyComponent</component-type>
			<renderer-type>test.outjected.renderers.CopyRenderer</renderer-type>
		</component>
		<attribute>
			<description>id</description>
			<name>id</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Text value to be copied</description>
			<name>value</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Copy Class</description>
			<name>copyClass</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Copied Class</description>
			<name>copiedClass</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>CSS Style </description>
			<name>style</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>CSS Style Classes</description>
			<name>styleClass</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Type of the Button (submit,button,reset). Default is button</description>
			<name>type</name>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<description>Renders a simple HTML Div</description>
		<tag-name>div</tag-name>
		<component>
			<component-type>test.outjected.components.DivComponent</component-type>
			<renderer-type>test.outjected.renderers.DivRenderer</renderer-type>
		</component>
		<attribute>
			<description>id</description>
			<name>id</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>CSS Style </description>
			<name>style</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>CSS Style Classes</description>
			<name>styleClass</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>onclick</description>
			<name>onclick</name>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<description>Renders a Select2 Autocomplete Component</description>
		<tag-name>select2AutoComplete</tag-name>
		<component>
			<component-type>test.outjected.components.Select2AutoCompleteComponent</component-type>
			<renderer-type>test.outjected.renderers.Select2AutoCompleteRenderer</renderer-type>
		</component>
		<attribute>
			<description>id</description>
			<name>id</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>value</description>
			<name>value</name>
			<required>true</required>
			<type>java.lang.Object</type>
		</attribute>
		<attribute>
			<description>placeholder</description>
			<name>placeholder</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>required</description>
			<name>required</name>
			<type>boolean</type>
		</attribute>
		<attribute>
			<description>initPath</description>
			<name>initPath</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>searchPath</description>
			<name>searchPath</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>converter</description>
			<name>converter</name>
			<required>false</required>
			<type>javax.faces.convert.Converter</type>
		</attribute>
	</tag>
	<tag>
		<tag-name>decorateInput</tag-name>
		<component>
			<component-type>test.outjected.components.DecorateInputComponent</component-type>
			<renderer-type>test.outjected.renderers.DecorateInputRenderer</renderer-type>
		</component>
		<attribute>
			<name>label</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>labelClass</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<display-name>CSS Styles</display-name>
			<name>style</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<display-name>CSS Style Classes</display-name>
			<name>styleClass</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>help</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>helpContainer</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>helpDelay</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>helpHtml</name>
			<type>boolean</type>
		</attribute>
		<attribute>
			<name>helpPlacement</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>helpTitle</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>helpTrigger</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>skipControlClass</name>
			<type>boolean</type>
		</attribute>
		<attribute>
			<name>valueClass</name>
			<type>java.lang.String</type>
		</attribute>
	</tag>
</facelet-taglib>