<facelet-taglib version="2.2"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd">
	<namespace>http://outjected.com/jsf/foo</namespace>
	<tag>
		<description>Renders a simple Button</description>
		<tag-name>div</tag-name>
		<component>
			<component-type>com.outjected.jsf.foo.components.ButtonComponent</component-type>
			<renderer-type>com.outjected.jsf.renderers.ButtonRenderer</renderer-type>
		</component>
		<attribute>
			<description>id</description>
			<name>id</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>CSS Style </description>
			<name>style</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>CSS Style Classes</description>
			<name>styleClass</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>onclick</description>
			<name>onclick</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Type of the Button (submit,button,reset). Default is button</description>
			<name>type</name>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<description>Renders a simple HTML Div</description>
		<tag-name>div</tag-name>
		<component>
			<component-type>com.outjected.jsf.foo.components.DivComponent</component-type>
			<renderer-type>com.outjected.jsf.renderers.DivRenderer</renderer-type>
		</component>
		<attribute>
			<description>id</description>
			<name>id</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>CSS Style </description>
			<name>style</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>CSS Style Classes</description>
			<name>styleClass</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>onclick</description>
			<name>onclick</name>
			<type>java.lang.String</type>
		</attribute>
	</tag>
</facelet-taglib>