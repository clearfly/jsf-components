			<required>false</required>
		</attribute>
		<attribute>
			<description>CSS Style Classes</description>
			<name>styleClass</name>
			<type>java.lang.String</type>
			<required>false</required>
		</attribute>
		<attribute>
			<description>onclick</description>
			<name>onclick</name>
			<type>java.lang.String</type>
			<required>false</required>
		</attribute>
